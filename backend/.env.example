# Server Configuration
PORT=3000
FRONTEND_URL=http://localhost:5173

# AWS Configuration
AWS_REGION=us-west-1
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
S3_BUCKET_NAME=your-s3-bucket-name

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key

# JWT Secrets (generate with: openssl rand -hex 64)
ACCESS_TOKEN_SECRET=your_access_token_secret
REFRESH_TOKEN_SECRET=your_refresh_token_secret
ACCESS_TOKEN_EXPIRY=15m
REFRESH_TOKEN_EXPIRY=30d

# Redis Configuration
REDIS_URL=redis://localhost:6379

# AWS Lambda & EventBridge Configuration
EXPIRE_TASK_LAMBDA_ARN=arn:aws:lambda:region:account-id:function:expireTask
EVENTBRIDGE_ROLE_ARN=arn:aws:iam::account-id:role/eventbridge-expireTask-role
SCHEDULE_GROUP=task-expiration-schedules

# Stripe Configuration
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_stripe_webhook_secret

# Payment API Key (generate with: openssl rand -hex 32)
PAYMENT_API_KEY=your_payment_api_key

# Admin/Reviewer Configuration
REVIEWER_EMAIL=your_reviewer_email@example.com
